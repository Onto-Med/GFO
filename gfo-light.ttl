#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/gfo/light/after
:after rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :temporallyRelatedWith ;
       owl:inverseOf :before ;
       rdfs:label "after"@en .


###  https://w3id.org/gfo/light/attributiveOf
:attributiveOf rdf:type owl:ObjectProperty ;
               owl:inverseOf :hasAttributive ;
               rdfs:domain :Attributive ;
               rdfs:range :Individual ;
               rdfs:label "attributive of"@en .


###  https://w3id.org/gfo/light/before
:before rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :temporallyRelatedWith ;
        rdfs:label "before"@en ;
        skos:definition "X before Y: Xf < Ys"@en .


###  https://w3id.org/gfo/light/boundaryOf
:boundaryOf rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasBoundary ;
            rdfs:domain :ImmaterialContinuant ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Line
                                       :MaterialObject
                                       :Surface
                                     )
                       ] ;
            rdfs:comment "Immaterial continuants can be considered as (outer or inner) boundaries (boundaryOf). Note that surfaces are boundaries of material continuants, lines are boundaries of surfaces and points are boundaries of lines."@en ;
            rdfs:label "boundary of"@en .


###  https://w3id.org/gfo/light/causedBy
:causedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :resultsFrom ;
          owl:inverseOf :causes ;
          rdfs:label "caused by"@en .


###  https://w3id.org/gfo/light/causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :leadsTo ;
        rdfs:label "causes"@en .


###  https://w3id.org/gfo/light/containsRole
:containsRole rdf:type owl:ObjectProperty ;
              owl:inverseOf :roleIn ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Continuant
                                          :Process
                                          :Relator
                                          :Situation
                                        )
                          ] ;
              rdfs:range :Role ;
              rdfs:label "contains role"@en .


###  https://w3id.org/gfo/light/during
:during rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :temporallyRelatedWith ;
        owl:inverseOf :temporallyContains ;
        rdfs:label "during"@en ;
        skos:definition "X during Y: Xs > Ys and Xf < Yf"@en .


###  https://w3id.org/gfo/light/existsAt
:existsAt rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasTime ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Attributive
                                      :Situation
                                    )
                      ] ;
          rdfs:range :TimeBoundary ;
          rdfs:label "exists at"@en .


###  https://w3id.org/gfo/light/existsIn
:existsIn rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasTime ;
          rdfs:range :Chronoid ;
          rdfs:label "exists in"@en .


###  https://w3id.org/gfo/light/hasAttributive
:hasAttributive rdf:type owl:ObjectProperty ;
                rdfs:domain :Individual ;
                rdfs:range :Attributive ;
                rdfs:label "has attributive"@en .


###  https://w3id.org/gfo/light/hasBoundary
:hasBoundary rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Line
                                         :MaterialObject
                                         :Surface
                                       )
                         ] ;
             rdfs:range :ImmaterialContinuant ;
             rdfs:label "has boundary"@en .


###  https://w3id.org/gfo/light/hasInnerBoundary
:hasInnerBoundary rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasBoundary ;
                  owl:inverseOf :innerBoundaryOf ;
                  rdfs:label "has inner boundary"@en .


###  https://w3id.org/gfo/light/hasMember
:hasMember rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasPart ;
           owl:inverseOf :memberOf ;
           rdfs:domain :MaterialAggregate ;
           rdfs:range :MaterialContinuant ;
           rdfs:label "has member"@en .


###  https://w3id.org/gfo/light/hasNaturalBoundary
:hasNaturalBoundary rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasBoundary ;
                    owl:inverseOf :naturalBoundaryOf ;
                    rdfs:label "has natural boundary"@en .


###  https://w3id.org/gfo/light/hasOwner
:hasOwner rdf:type owl:ObjectProperty ;
          owl:inverseOf :ownerOf ;
          rdfs:domain :ImmaterialContinuant ;
          rdfs:range :MaterialContinuant ;
          rdfs:comment "Immaterial continuants can be assigned to a corresponding material continuant using the object property hasOwner."@en ;
          rdfs:label "has owner"@en .


###  https://w3id.org/gfo/light/hasPart
:hasPart rdf:type owl:ObjectProperty ;
         owl:inverseOf :partOf ;
         rdfs:domain :Individual ;
         rdfs:range :Individual ;
         rdfs:label "has part"@en .


###  https://w3id.org/gfo/light/hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :participatesIn ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Process
                                            :Situation
                                          )
                            ] ;
                rdfs:range :Continuant ;
                rdfs:label "has participant"@en .


###  https://w3id.org/gfo/light/hasProcessPart
:hasProcessPart rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasPart ;
                owl:inverseOf :processPartOf ;
                rdfs:domain :Process ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Attributive
                                           :Process
                                           :Situation
                                         )
                           ] ;
                rdfs:label "has process part"@en .


###  https://w3id.org/gfo/light/hasQuality
:hasQuality rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasAttributive ;
            owl:inverseOf :qualityOf ;
            rdfs:domain :Individual ;
            rdfs:range :Quality ;
            rdfs:label "has quality"@en .


###  https://w3id.org/gfo/light/hasSituationPart
:hasSituationPart rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasPart ;
                  owl:inverseOf :situationPartOf ;
                  rdfs:domain :Situation ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( :Attributive
                                             :Situation
                                           )
                             ] ;
                  rdfs:label "has situation part"@en .


###  https://w3id.org/gfo/light/hasTime
:hasTime rdf:type owl:ObjectProperty ;
         rdfs:domain :ConcreteIndividual ;
         rdfs:range :TimeEntity ;
         rdfs:label "has time"@en .


###  https://w3id.org/gfo/light/innerBoundaryOf
:innerBoundaryOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :boundaryOf ;
                 rdfs:label "inner boundary of"@en .


###  https://w3id.org/gfo/light/instanceOf
:instanceOf rdf:type owl:ObjectProperty ;
            owl:inverseOf :instantiatedBy ;
            rdfs:domain :Entity ;
            rdfs:range :Category ;
            rdfs:label "instance of"@en .


###  https://w3id.org/gfo/light/instantiatedBy
:instantiatedBy rdf:type owl:ObjectProperty ;
                rdfs:domain :Category ;
                rdfs:range :Entity ;
                rdfs:label "instantiated by"@en .


###  https://w3id.org/gfo/light/leadsTo
:leadsTo rdf:type owl:ObjectProperty ;
         owl:inverseOf :resultsFrom ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Process
                                     :Situation
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Process
                                    :Situation
                                  )
                    ] ;
         rdfs:label "leads to"@en .


###  https://w3id.org/gfo/light/locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           owl:inverseOf :locationOf ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :MaterialObject
                                       :Process
                                       :Situation
                                     )
                       ] ;
           rdfs:range :MaterialObject ;
           rdfs:comment "Material objects can be located in other material objects (locatedIn, e.g., the driver is located in the car)."@en ,
                        "Processes and situations can take place (locatedIn) in material objects (e.g., a football match in a stadium or an illness in a person)."@en ;
           rdfs:label "located in"@en .


###  https://w3id.org/gfo/light/locationOf
:locationOf rdf:type owl:ObjectProperty ;
            rdfs:domain :MaterialObject ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :MaterialObject
                                       :Process
                                       :Situation
                                     )
                       ] ;
            rdfs:label "location of"@en .


###  https://w3id.org/gfo/light/memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :partOf ;
          rdfs:domain :MaterialContinuant ;
          rdfs:range :MaterialAggregate ;
          rdfs:comment "To model the membership of a member in a group/aggregate, the more specific object properties hasMember/memberOf should be used instead of hasPart/PartOf."@en ;
          rdfs:label "member of"@en .


###  https://w3id.org/gfo/light/naturalBoundaryOf
:naturalBoundaryOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :boundaryOf ;
                   rdfs:label "natural boundary of"@en .


###  https://w3id.org/gfo/light/ownerOf
:ownerOf rdf:type owl:ObjectProperty ;
         rdfs:domain :MaterialContinuant ;
         rdfs:range :ImmaterialContinuant ;
         rdfs:label "owner of"@en .


###  https://w3id.org/gfo/light/partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :Individual ;
        rdfs:range :Individual ;
        rdfs:label "part of"@en .


###  https://w3id.org/gfo/light/participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Continuant ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Process
                                           :Situation
                                         )
                           ] ;
                rdfs:comment "Continuants can participate in processes and situations (participatesIn, hasParticipant)."@en ;
                rdfs:label "participates in"@en ;
                skos:example "Player A1 participates in a goal situation."@en ,
                             "Teams A and B participate in a football match."@en .


###  https://w3id.org/gfo/light/playedBy
:playedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :attributiveOf ;
          owl:inverseOf :plays ;
          rdfs:domain :Role ;
          rdfs:range :Continuant ;
          rdfs:label "played by"@en .


###  https://w3id.org/gfo/light/plays
:plays rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :hasAttributive ;
       rdfs:domain :Continuant ;
       rdfs:range :Role ;
       rdfs:comment "The role players are associated with the roles they play using object properties plays or playedBy."@en ;
       rdfs:label "plays"@en .


###  https://w3id.org/gfo/light/processPartOf
:processPartOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :partOf ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Attributive
                                           :Process
                                           :Situation
                                         )
                           ] ;
               rdfs:range :Process ;
               rdfs:label "process part of"@en .


###  https://w3id.org/gfo/light/qualityOf
:qualityOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :attributiveOf ;
           rdfs:domain :Quality ;
           rdfs:range :Individual ;
           rdfs:label "quality of"@en .


###  https://w3id.org/gfo/light/relatedBy
:relatedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAttributive ;
           owl:inverseOf :relates ;
           rdfs:domain :Continuant ;
           rdfs:range :Relator ;
           rdfs:label "related by"@en .


###  https://w3id.org/gfo/light/relates
:relates rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :attributiveOf ;
         rdfs:domain :Relator ;
         rdfs:range :Continuant ;
         rdfs:label "relates"@en .


###  https://w3id.org/gfo/light/resultsFrom
:resultsFrom rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Process
                                         :Situation
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Process
                                        :Situation
                                      )
                        ] ;
             rdfs:label "results from"@en .


###  https://w3id.org/gfo/light/roleIn
:roleIn rdf:type owl:ObjectProperty ;
        rdfs:domain :Role ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Continuant
                                   :Process
                                   :Relator
                                   :Situation
                                 )
                   ] ;
        rdfs:comment "Roles are assigned to the corresponding context entity (continuant, process, situation, relator) using object properties roleIn or containsRole."@en ;
        rdfs:label "role in"@en .


###  https://w3id.org/gfo/light/situationPartOf
:situationPartOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :partOf ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Attributive
                                             :Situation
                                           )
                             ] ;
                 rdfs:range :Situation ;
                 rdfs:label "situation part of"@en .


###  https://w3id.org/gfo/light/spatiallyEndsOn
:spatiallyEndsOn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :spatiallyPassesThrough ;
                 rdfs:label "spatially ends on"@en .


###  https://w3id.org/gfo/light/spatiallyPassesThrough
:spatiallyPassesThrough rdf:type owl:ObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Line
                                                    :Surface
                                                  )
                                    ] ;
                        rdfs:range [ rdf:type owl:Class ;
                                     owl:unionOf ( :ImmaterialContinuant
                                                   :MaterialObject
                                                 )
                                   ] ;
                        rdfs:label "spatially passes through"@en .


###  https://w3id.org/gfo/light/spatiallyStartsOn
:spatiallyStartsOn rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :spatiallyPassesThrough ;
                   rdfs:label "spatially starts on"@en .


###  https://w3id.org/gfo/light/temporallyContains
:temporallyContains rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :temporallyRelatedWith ;
                    rdfs:label "temporally contains"@en .


###  https://w3id.org/gfo/light/temporallyEqual
:temporallyEqual rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 rdfs:label "temporally equal"@en ;
                 skos:definition "X equal Y: Xs = Ys and Xf = Yf"@en .


###  https://w3id.org/gfo/light/temporallyFinishedBy
:temporallyFinishedBy rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :temporallyRelatedWith ;
                      owl:inverseOf :temporallyFinishes ;
                      rdfs:label "temporally finished by"@en .


###  https://w3id.org/gfo/light/temporallyFinishes
:temporallyFinishes rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :temporallyRelatedWith ;
                    rdfs:label "temporally finishes"@en ;
                    skos:definition "X finishes Y: Xs > Ys and Xf = Yf"@en .


###  https://w3id.org/gfo/light/temporallyMeets
:temporallyMeets rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 owl:inverseOf :temporallyMetBy ;
                 rdfs:label "temporally meets"@en ;
                 skos:definition "X meets Y: Xf = Ys"@en .


###  https://w3id.org/gfo/light/temporallyMetBy
:temporallyMetBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 rdfs:label "temporally met by"@en .


###  https://w3id.org/gfo/light/temporallyOverlappedBy
:temporallyOverlappedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :temporallyRelatedWith ;
                        owl:inverseOf :temporallyOverlaps ;
                        rdfs:label "temporally overlapped by"@en .


###  https://w3id.org/gfo/light/temporallyOverlaps
:temporallyOverlaps rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :temporallyRelatedWith ;
                    rdfs:label "temporally overlaps"@en ;
                    skos:definition "X overlaps Y: Xs < Ys and Xf > Ys and Xf < Yf"@en .


###  https://w3id.org/gfo/light/temporallyRelatedWith
:temporallyRelatedWith rdf:type owl:ObjectProperty ;
                       rdfs:domain :Individual ;
                       rdfs:range :Individual ;
                       rdfs:comment "If a temporal relation is applied to other concrete individuals as time intervals (attributive, continuant, process, situation), it is propagated to their existence times (hasTime)."@en ,
                                    "In the definitions of the relations, the variables Xs, Xf, Ys and Yf represent the start (s) and finish (f) of the time intervals X and Y."@en ,
                                    "Some temporal relations can also be applied to points in time (before/after, equal) or a point in time and a time interval (before, starts, finishes, during or their inverse relations)."@en ;
                       rdfs:label "temporally related with"@en ;
                       skos:definition "Temporal relations between time intervals based on Allen, J. F. (1983). Maintaining knowledge about temporal intervals. Communications of the ACM, 26(11), 832-843."@en .


###  https://w3id.org/gfo/light/temporallyStartedBy
:temporallyStartedBy rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :temporallyRelatedWith ;
                     owl:inverseOf :temporallyStarts ;
                     rdfs:label "temporally started by"@en .


###  https://w3id.org/gfo/light/temporallyStarts
:temporallyStarts rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :temporallyRelatedWith ;
                  rdfs:label "temporally starts"@en ;
                  skos:definition "X starts Y: Xs = Ys and Xf < Yf"@en .


