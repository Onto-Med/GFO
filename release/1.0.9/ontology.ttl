                                      owl:onProperty :value_of ;
                                      owl:someValuesFrom :Property
                                    ] ;
                rdfs:subClassOf :Concrete ,
                                :Dependent ;
                rdfs:comment "The concept of a property value reflects a relationship between the property of x and the same property as exhibited by another entity y." .


###  http://www.onto-med.de/ontologies/gfo.owl#Relational_role
:Relational_role rdf:type owl:Class ;
                 rdfs:subClassOf :Dependent ,
                                 :Role ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :role_of ;
                                   owl:someValuesFrom :Relator
